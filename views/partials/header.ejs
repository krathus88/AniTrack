                                            <h5 class="card-title bold-text text-truncate"></h5>
                                            <p class="info-text light-text"></p>
                                        </div>
                                    </button>
                                    <button type="submit" class="card flex-row w-100 p-0 text-start" id="result2" name="animeID" value="" style="display: none;">
                                        <img src="" class="card-img-top img-search-preview" alt="" />
                                        <div class="p-0 mt-2 mx-3 text-truncate">
                                            <h5 class="card-title bold-text text-truncate"></h5>
                                            <p class="info-text light-text"></p>
                                        </div>
                                    </button>
                                    <button type="submit" class="card flex-row w-100 p-0 text-start" id="result3" name="animeID" value="" style="display: none;">
                                        <img src="" class="card-img-top img-search-preview" alt="" />
                                        <div class="p-0 mt-2 mx-3 text-truncate">
                                            <h5 class="card-title bold-text text-truncate"></h5>
                                            <p class="info-text light-text"></p>
                                        </div>
                                    </button>
                                </div>
                            </form>
                            <script src="https://cdn.jsdelivr.net/npm/axios@1.1.2/dist/axios.min.js"></script>
                            <script>
                                const searchInput = document.getElementById('animeSearch');
                                const searchResultsContainer = document.getElementById('searchResultsContainer');

                                let searchTimeout;

                                // Function to close search results container
                                const closeSearchResults = () => {
                                    searchResultsContainer.style.display = 'none';
                                };

                                // Function to open search results container
                                const openSearchResults = () => {
                                    if (searchInput.value.trim() !== '') {
                                        searchResultsContainer.style.display = 'block';
                                    }
                                };

                                // Click event listener to close search results when clicking outside
                                document.addEventListener('click', function(event) {
                                    if (!searchResultsContainer.contains(event.target) && event.target !== searchInput) {
                                        closeSearchResults();
                                    }
                                });

                                // Focus event listener to open search results when focusing on search input
                                searchInput.addEventListener('focus', openSearchResults);

                                searchInput.addEventListener('input', function(event) {
                                    clearTimeout(searchTimeout);

                                    const searchTerm = event.target.value.trim();

                                    if (!searchTerm) {
                                        // Hide all search result divs
                                        document.getElementById('result1').style.display = 'none';
                                        document.getElementById('result2').style.display = 'none';
                                        document.getElementById('result3').style.display = 'none';
                                        return;
                                    }

                                    openSearchResults();

                                    searchTimeout = setTimeout(async function() {
                                        try {
                                            const response = await axios.get(`https://api.jikan.moe/v4/anime?q=${searchTerm}`);
                                            const searchResults = response.data.data.slice(0, 3); // Get top 3 results

                                            // Update search result divs with data
                                            searchResults.forEach((result, index) => {
                                                const resultDiv = document.getElementById(`result${index + 1}`);
                                                resultDiv.value = result.mal_id;
                                                resultDiv.style.display = 'flex'; // Make the div visible
                                                resultDiv.querySelector('.img-search-preview').src = result.images.jpg.large_image_url;
                                                resultDiv.querySelector('.img-search-preview').alt = findAnimeTitle(result);
                                                resultDiv.querySelector('.card-title').innerText = findAnimeTitle(result);
                                                resultDiv.querySelector('.info-text').innerText = result.rating + " | " + "Score: " + result.score;
                                            });
                                        } catch (error) {
                                            console.error(error);
                                        }
                                    }, 1000); // Wait for 1 second after typing stops
                                });

                                const findAnimeTitle = (data) => {
                                    const englishTitle = data.titles.find(title => title.type === "English");
                                    const defaultTitle = data.titles.find(title => title.type === "Default");

                                    if (englishTitle) {
                                        return englishTitle.title;
                                    } else if (defaultTitle) {
                                        return defaultTitle.title;
                                    } else {
                                        return "Title not found";
                                    }
                                };
                            </script>
