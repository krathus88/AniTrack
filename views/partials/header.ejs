                            <!-- Search Results Container -->
                            <form action="/anime" method="post">
                                <div id="searchResultsContainer" class="position-absolute mt-1 col-11 col-md-6 col-lg-4 mb-3 mb-lg-0" style="z-index: 100;">
                                    <!-- Placeholder divs for search results -->
                                    <button type="submit" class="card flex-row w-100 p-0 text-start" id="result1" name="animeID" value="" style="display: none;">
